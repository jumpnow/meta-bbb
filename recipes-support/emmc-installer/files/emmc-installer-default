# Default location of binaries to install onto the eMMC when the
# emmc_installer.sh script is run from the emmc-installer service 
SRCDIR=/home/root/emmc

# The partitioning script to use: emmc_mk2parts.sh or emmc_mk5parts.sh
# Set to emmc_mk5parts.sh if using the emmc-upgrader package.
PARTITION_SCRIPT=/usr/bin/emmc_mk2parts.sh
